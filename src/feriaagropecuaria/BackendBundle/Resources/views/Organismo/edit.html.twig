{% extends "::adminlayaout.html.twig" %}

{% block titulo -%}Crear Organismo{% endblock %}
{% block body -%}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3>Editar Organismo</h3>
        </div>
        <div class="panel-body">
            {{ form_start(edit_form) }}

            {{ form_row(edit_form.Nombre) }}
            <br/>
            <label>Nombre en Ingles</label>
            <input name="NombreENG" type="text" class="form-control" required="true">
            <br/>

            {{ form_row(edit_form.submit) }}

            {{ form_end(edit_form) }}

        </div>
    </div>

        <ul class="record_actions">
    <li>
        <a href="{{ path('organismo') }}">
            Back to the list
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
