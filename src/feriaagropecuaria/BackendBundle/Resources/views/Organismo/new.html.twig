{% extends "::adminlayaout.html.twig" %}

{% block titulo -%}Crear Organismo{% endblock %}
{% block body -%}
<div class="panel panel-default">
    <div class="panel-heading">
        <h3>Nuevo Organismo</h3>
    </div>
    <div class="panel-body">
        {{ form_start(form) }}

        {{ form_row(form.Nombre) }}
        <br/>
        <label>Nombre en Ingles</label>
        <input name="NombreENG" type="text" class="form-control" required="true">
        <br/>

        {{ form_row(form.submit) }}

        {{ form_end(form) }}

    </div>
</div>


        <ul class="record_actions">
    <li>
        <a href="{{ path('organismo') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}

