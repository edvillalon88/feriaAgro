{% extends "::adminlayaout.html.twig" %}

{% block titulo -%}Servicio, Mobiliaria y Producto list{% endblock %}

{% block body -%}

<div class="table-responsive">
    <table class="records_list table table-striped">

        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('smp_show', { 'id': entity.idServicioMobiliariaProducto }) }}">{{ entity.idServicioMobiliariaProducto }}</a></td>
                <td>{{ entity.Nombre }}</td>
                <td>{{ entity.Precio }}</td>
                <td><img src="{{ asset(entity.webPath) }}" /></td>
                <td>

                        <a href="{{ path('smp_show', { 'id': entity.idServicioMobiliariaProducto }) }}"><button type="button" class="btn btn-default">show</button></a>

                        <a href="{{ path('smp_edit', { 'id': entity.idServicioMobiliariaProducto }) }}"><button type="button" class="btn btn-default">edit</button></a>

                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>

            <a href="{{ path('smp_new') }}">
                <button type="button" class="btn btn-primary">Create a new entry</button>
            </a>

    {% endblock %}
