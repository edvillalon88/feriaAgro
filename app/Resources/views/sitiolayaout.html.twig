<!-- Asynchronous Google Analytics snippet. Change UA-XXXXX-X to be your site's ID.
     mathiasbynens.be/notes/async-analytics-snippet -->

<script>


    $(document).ready(function(){


        $('.'+'{{  app.request.locale }}').addClass('active');
        sizeUpdate();
        var tgt = location.hash.replace(/^#\/?/,'');
        $.smoothScroll({scrollTarget: '#' + tgt});
        //$('#scrollto4').smoothScroll();
        $('.smootScroll').smoothScroll();

        //color box solo para tablets y moviles
        if(detectPantalla() == "movil" || detectPantalla() == "tablet"){

            $(".galeriaSmall a").colorbox({rel:'fotos', transition:"fade"});
        }

        $(window).resize(function() {
            sizeUpdate();
        });

        $("#toggleMenu").click(function(e){
            e.preventDefault();

            if($("header  nav").css("display") == "none"){
                $("header  nav").show();
                if(detectPantalla() == "movil" || detectPantalla() == "tablet"){
                    $("#main").hide();
                    $(".redes").hide();
                    $("header").css("position","absolute");
                    $("footer").hide();
                    if(detectPantalla() == "tablet") $("header nav >ul").css("height","auto");
                    if(detectPantalla() == "movil") $("header nav >ul").css("height","auto");
                }
            } else {
                $("header  nav").hide();
                if(detectPantalla() == "movil" || detectPantalla() == "tablet"){
                    $("#main").show();
                    $(".redes").show();
                    $("header").css("position","fixed");
                    $("footer").show();
                }
            }

        });
        $('.slider1').bxSlider({
            minSlides: 1,
            maxSlides: 1,
            pager: false,
            nextText: '>',
            prevText: '<',
            slideMargin: 10
        });
        $(".imagenes").click(function(){
            $(".imagenes").css("z-index","2");
            $(this).css("z-index","3");
        });

        $("#cancelar").click(function(){
            $("#identificador").val("");
        });
        $("#enviar").click(function(){

            var iden = $("#identificador").val();
            if(iden != "")
            {
                $.post("{{ path("verificar_solicitud") }}", { identificador: iden },
                        function(data){
                            if(data=="true")
                            {
                                if(iden != "")
                                {

                                    window.location = "{{ path("solicitud_participacion_show",{ '_locale': app.request.locale  } ) }}/"+iden;
                                }
                            }else
                            {
                                alert("Su solicitud no existe");
                            }

                        });
            }




        });

        //document ready
    });//------------
    //document ready

</script>

